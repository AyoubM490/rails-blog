<div class="form">
  <%= form_with model: @post, url: user_posts_url, method: :post do |form| %>
    <div class="form-group">
      <%= form.text_field :title, :placeholder => "Title" %>
      <%= form.text_field :text, :placeholder => "Text" %>
      <%= form.submit "Submit" %>
    </div>
  <% end %>
</div>
